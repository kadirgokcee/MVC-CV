@model MvcCv.Models.Entity.TblSertifikalarim
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Sertifika Ekleme Sayfası</h4>
<br />
<form method="post">
    <br />
    @Html.Label("Sertifika Açıklaması")
    @Html.TextBoxFor(x => x.Aciklam, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>